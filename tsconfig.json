{
    "compilerOptions": {
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true,
        "typeRoots": [
            "node_modules/@types",
            "typings"
        ],
        "newLine": "LF",
        "jsx": "react"
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
